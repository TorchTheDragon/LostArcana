{
  "schemaVersion": 1,
  "id": "lostarcana",
  "version": "${version}",

  "name": "Lost Arcana",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/lostarcana/icon.png",

  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      {
        "adapter": "kotlin",
        "value": "com.oyosite.ticon.lostarcana.datagen.DataGenEntrypoint"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.oyosite.ticon.lostarcana.LostArcana"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.oyosite.ticon.lostarcana.LostArcanaClient"
      }
    ]
  },
  "mixins": [
    "lostarcana.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "~${minecraft_version}",
    "java": ">=17",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}"
  },
  "suggests": {
    "another-mod": "*"
  }
}
